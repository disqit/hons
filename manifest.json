{
  "name": "AdAnalysis",
  "description": "Crawl web pages to determine advert prominence",
  "version": "1.0",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popup.js"]
    }
  ],
  "browser_action": {
      "default_title": "Crawl a page",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2
}